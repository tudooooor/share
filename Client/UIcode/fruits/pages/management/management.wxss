@import '/lib/style/stylesheet.wxss';

page{
  background-color: #fff;
}
.standard{
  margin-top: 15px;
  justify-content: space-between;
}

.manage_left,.manage_right{
  width: 47%;
  background-color: #f8f8f8;
  height: 200rpx;
  border: 1px solid #f4f4f4;
  border-radius: 4px;
}
.mark{
  width: 100%;
  height: 120rpx;
  line-height: 150rpx;
  text-align: center;
}
.mark::after{
  font-family: 'iconfont';
  content: '\e608';
  font-size: 100rpx;
  color: #999;

}
.text{
  text-align: center;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 30rpx;
  color: #333;
  font-weight: 400;
}