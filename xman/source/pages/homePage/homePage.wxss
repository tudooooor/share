#channelSlider {
    max-width: 640px;
    position: fixed;
    height: 46.4px;
    top: 0px;
    width: 100%;
    z-index: 10015;
    background-color: #fff;
    overflow: hidden;
    border-top: 1px solid #eceded;
    border-bottom: 1px solid #eceded
}

#channelSlider .ch-left {
    background-image: linear-gradient(270deg,hsla(0,0%,100%,0) 0,#fff);
    width: 35.2rpx;
    float: left
}

#channelSlider .ch-left,#channelSlider .ch-right {
    z-index: 10015;
    height: inherit;
    overflow: hidden;
    position: absolute
}

#channelSlider .ch-right {
    background-image: linear-gradient(90deg,hsla(0,0%,100%,0) 0,#fff);
    /*width: 28.8px;*/
    right: 0;
    float: right
}

#channelSlider .ch-content {
    position: relative;
    height: 46.4px;
    font-size: 15px;
    font-family: Hiragino Sans GB;
    color: #626262;
    text-align: center;
    top: 0;
    white-space: nowrap;
    overflow-x: auto;
    overflow-y: hidden;
    padding: 0 2.56px;
    z-index: 10010;
    -webkit-overflow-scrolling: touch
}

#channelSlider .ch-content .li:first-child {
    margin-left: 16px;
}

#channelSlider .ch-content .ch-tab-item {
    height: 46.4px;
    display: inline-block;
    margin-left: 20px;
    line-height: 46.4px;
    text-align: center;
    z-index: 10005
}

#channelSlider .ch-content .cur-ch-tab {
    border-bottom: 2px solid #e00;
    color: #e00;
    height: 40px;
    display: inline-block;
    position: relative;
    z-index: 10005
}

#channelSlider .ch-content .ch-tab {
    color: #626262;
    height: 46.4px;
    display: inline-block;
    position: relative;
    z-index: 10005
}

/*#channelSlider .ch-content .li:first-child {
