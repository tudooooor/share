#main {
    color: #666;
    font-family: Helvetica,STHeiti STXihei,Microsoft JhengHei,Microsoft YaHei,Arial;
    font-size: 12px;
    line-height: 1.5;
}

.p {display: inline}

#main,.div.navbar,.header {
    margin: auto;
    max-width: 640px
}
.information{
  width: 62%;
  margin-left: 3%;
  display: flex;
  flex-direction: column;
}

.discount::after{
  font-family: 'iconfont';
  content: "\e610";
  color: #ffb200;
  font-size: 65rpx;
  position: absolute;
  top: -2rpx;
  left: -5rpx;
}

.shop_name{
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 32rpx;
  font-weight: 300 ;
  color: #333;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.comprehensive_situation,.rules,.vip{
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
}
.com_left{
  width: 65%;
  height: 45rpx;
  line-height: 45rpx;
  display: flex;
  flex-direction: row;
}
.rank,.Starting_price{
  width: 40%;
  font-size: 24rpx;
  font-weight: 500;
}
.flower{
  color: #fa3137;
}
.flower::after{
  font-family: 'iconfont';
  content: '\e604';
  color: #fa3137;
  font-size: 24rpx;
  font-weight: bold;
}
.Starting_price,.delivery{
  color: #999;
  font-size: 24rpx;
}
.sales,.delivery{
  width: 60%;
 
  font-weight: 500;
  /* font: 24rpx #333  400 ; */
}
.sales{
  margin-left: 7%;
  color: #333;
  font-size: 28rpx;
  font-weight: 400;
}
.com_right{
  width: 35%;
  height: 45rpx;
  line-height: 45rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: #333;
  text-align: right;
}
.com_right1{
  width: 35%;
  height: 45rpx;
  line-height: 45rpx;
  text-align: right;
}
.com_right1::after{
  font-family: 'iconfont';
  content: '\e6c2';
  color: #5bbc04;
  font-size: 45rpx;
  font-weight: 500;
  padding: 0 4rpx;
}
.vip_price{
  width: 100%;
  font-size: 24rpx;
  color: #5bbc04;
  font-weight: 400;
}

.swiper,swiper image{
