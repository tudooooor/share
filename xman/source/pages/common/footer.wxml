<view class="div">
<view id="pin-spinner" class="div pin-spinner-2" style="{{page.load ? 'display: none' : 'display: block'}}; width: 66px;">
    <view class="div" id="ps-background" style="{{page.operate != 1 ? 'display: none' : 'display: block'}}"></view>
    <view class="div ps-inner-box">
        <image id="imgInit" class="ps-rotate-img" src="data:image/png;base64," style="bottom: 0px;"/>
    </view>
    <view id="msgInit" class="div ps-rotate-msg" style="display: none;"></view>
</view>

<view class="div pin-toast-container" style="{{toast.toast ? 'display: block;opacity: toast.toast;' : 'display: none;opacity: 0;'}}">
    <view class="div pin-toast" style="{{toast.toast ? 'display: inline-block;opacity: toast.toast;' : 'display: none;opacity: 0;'}}">{{toast.mess}}</view>
</view>

</view>



<view class="div" id="main-error" hidden="{{!(notNetCon.error && notNetCon.show && notNetCon.is_ajax)}}">
	<view class="div error-img"></view>
	<view class="error-msg">服务器君吃撑了~请再试试</view>
	<view class="div error-btn" bindtap="refresh">刷新看看</view>
</view>


<view class="div" id="main-error" hidden="{{!(notNetCon.error && notNetCon.show && !notNetCon.is_ajax)}}">
	<view class="div error-noimage"></view>
	<view class="error-msg">手机网络不太流畅，请检查网络状况</view>
	<view class="div error-btn" bindtap="refresh">重新加载</view>
</view>

